{
    "name": "tagtaste/api",
    "description": "The TagTaste API.",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "zizaco/entrust": "5.2.x-dev",
        "laravel/socialite": "^2.0",
        "doctrine/dbal": "^2.5",
        "laravelcollective/html": "^5.3.0",
        "socialiteproviders/instagram": "^2.0",
        "predis/predis": "~1.0",
        "elasticsearch/elasticsearch": "~5.0",
        "tymon/jwt-auth": "0.5.*",
        "guzzlehttp/guzzle": "^6.2",
        "tagtaste/api-response": "~1.1.0"

    },

    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laralib/l5scaffold":  "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/cjwilburn/l5scaffold.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/TagTaste/api-response.git"
        }

    ],

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Test\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
