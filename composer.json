{
    "name": "tagtaste/api",
    "description": "The TagTaste API.",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "zizaco/entrust": "5.2.x-dev",
        "doctrine/dbal": "^2.5",
        "laravelcollective/html": "^5.3.0",
        "predis/predis": "~1.0",
        "elasticsearch/elasticsearch": "~5.0",
        "tymon/jwt-auth": "0.5.*",
        "guzzlehttp/guzzle": "^6.2",
        "tagtaste/api-response": "~1.1.0",
        "laravel/socialite": "^3.0",
        "stevebauman/purify": "^1.1",
        "maatwebsite/excel": "2.1.19",
        "league/flysystem-aws-s3-v3": "~1.0",
        "revolution/laravel-google-sheets": "^2.3",
        "tagtaste/laravel-fcm": "^1.3",
        "php-amqplib/php-amqplib": "v2.7.0",
        "spatie/laravel-image-optimizer": "^1.1",
        "intervention/image": "^2.4",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "webpatser/laravel-uuid": "2.*"
    },

    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laralib/l5scaffold":  "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/cjwilburn/l5scaffold.git"
        },
        {
          "type": "vcs",
          "url":  "https://github.com/TagTaste/api-response.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/TagTaste/Laravel-FCM.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        { "packagist": false }

    ],

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
